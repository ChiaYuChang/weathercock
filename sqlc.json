{
    "version": "2",
    "sql": [
        {
            "schema": "schema.sql",
            "queries": "queries/*.sql",
            "engine": "postgresql",
            "gen": {
                "go": {
                    "package": "models",
                    "out": "internal/models",
                    "sql_package": "pgx/v5",
                    "emit_db_tags": true,
                    "emit_json_tags": true,
                    "emit_prepared_queries": true,
                    "emit_interface": true,
                    "emit_enum_valid_method": true,
                    "json_tags_case_style": "snake",
                    "overrides": [
                        {
                            "db_type": "uuid",
                            "go_type": "github.com/google/uuid.UUID"
                        },
                        {
                            "db_type": "timestamptz",
                            "go_type": "time.Time"
                        }
                    ]
                }
            }
        }
    ]
}